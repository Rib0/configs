{
    "compilerOptions": {
        "baseUrl": ".",
        "noImplicitAny": true,
        "allowJs": false,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "skipLibCheck": true,
        "paths": {
            "@/*": ["src/*"]
        }
    }
}
