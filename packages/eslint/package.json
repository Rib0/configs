{
  "name": "@rib0/eslint-config",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "yarn@4.9.0",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "imports": {
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.js",
      "./src/*/index.ts",
      "./src/*/index.js"
    ],
    "#dist/*": "./dist/*"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js"
  },
  "sideEffects": false,
  "author": {
    "name": "Nikita Sidenko",
    "url": "https://www.instagram.com/nikitas.222/"
  },
  "description": "eslint config",
  "scripts": {
    "build": "tsup",
    "check:types": "attw --pack .",
    "test": "node ./dist/index.js && eslint --fix ./src/test/index.js ./src/test/index.ts --config ./src/test/eslint.config.js",
    "prepack": "yarn build && yarn test"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Rib0/configs",
    "directory": "packages/eslint"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "eslint": "9.25.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "8.32.0",
    "globals": "^16.0.0"
  },
  "peerDependencies": {
    "@eslint/js": ">=9.25.0",
    "typescript": ">=5.8.3"
  }
}
